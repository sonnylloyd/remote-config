{
  "name": "sip-remote-config",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "nodemon --ext ts --exec ts-node src/server.ts",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build:sass": "sass --load-path=node_modules src/assets/styles/app.scss public/css/app.css",
    "watch:sass": "sass --watch --load-path=node_modules src/assets/styles/app.scss:public/css/app.css",
    "build:js": "esbuild src/assets/ts/scripts.ts --bundle --outfile=public/js/scripts.js --target=es6"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "awilix": "^12.0.4",
    "awilix-express": "^9.0.2",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-basic-auth": "^1.2.1",
    "nunjucks": "^3.2.4",
    "prettier": "^3.5.0",
    "qrcode": "^1.5.4",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/bootstrap": "^5.2.10",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "@types/nunjucks": "^3.2.6",
    "@types/qrcode": "^1.5.5",
    "@types/redis": "^4.0.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.20.1",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "sass": "^1.85.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  }
}
